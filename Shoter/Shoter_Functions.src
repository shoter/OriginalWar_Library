

Export function OWL_Shoter_Functions_Execute( {string} function_name, parameters );
begin
     case function_name of
          'pojaw_skrzynke' :
                        begin pojaw_skrzynke( parameters ); end;
                  'pojaw_sojusznika' :
                        begin pojaw_sojusznika( parameters ); end;
     end;
end;